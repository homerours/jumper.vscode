{
  "name": "jumper-vscode",
  "displayName": "Jumper for VSCode",
  "description": "Fast file and directory navigation with frecency algorithm. Quick jump to frequently and recently used files or directories. Fuzzy finder with smart ranking.",
  "version": "0.1.4",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumper.jumpToFile",
        "title": "Jumper: Jump to File"
      },
      {
        "command": "jumper.jumpToDirectory",
        "title": "Jumper: Jump to Directory"
      }
    ],
    "keybindings": [
      {
        "command": "jumper.jumpToFile",
        "key": "ctrl+alt+u",
        "mac": "ctrl+alt+u"
      },
      {
        "command": "jumper.jumpToDirectory",
        "key": "ctrl+alt+y",
        "mac": "ctrl+alt+y"
      }
    ],
    "configuration": {
      "title": "Jumper",
      "properties": {
        "jumper.maxResults": {
          "type": "number",
          "default": 300,
          "description": "Maximum number of results to show"
        },
        "jumper.beta": {
          "type": "number",
          "default": 1,
          "description": "Beta parameter for ranking algorithm"
        },
        "jumper.syntax": {
          "type": "string",
          "default": "extended",
          "enum": [
            "extended",
            "fuzzy"
          ],
          "description": "Query syntax mode"
        },
        "jumper.caseSensitivity": {
          "type": "string",
          "default": "default",
          "enum": [
            "default",
            "sensitive",
            "insensitive"
          ],
          "description": "Case sensitivity for queries"
        },
        "jumper.homeTilde": {
          "type": "boolean",
          "default": true,
          "description": "Substitute $HOME with ~/ in results"
        },
        "jumper.relative": {
          "type": "boolean",
          "default": false,
          "description": "Show relative paths"
        },
        "jumper.weights.visit": {
          "type": "number",
          "default": 1,
          "description": "Weight for opening a file"
        },
        "jumper.weights.manualSave": {
          "type": "number",
          "default": 0.3,
          "description": "Weight for manually saving a file (Ctrl/Cmd+S)"
        },
        "jumper.weights.autoSave": {
          "type": "number",
          "default": 0.05,
          "description": "Weight for auto-saving a file"
        },
        "jumper.weights.activeEditor": {
          "type": "number",
          "default": 0.3,
          "description": "Weight for switching to a file in the editor"
        },
        "jumper.excludePatterns": {
          "type": "array",
          "default": [
            "**/.git/**",
            "**/node_modules/**",
            "**/dist/**",
            "**/build/**",
            "**/out/**",
            "**/.vscode/**",
            "**/.idea/**",
            "**/coverage/**",
            "**/__pycache__/**",
            "**/target/**",
            "**/*.log",
            "**/*.tmp",
            "**/*.temp",
            "**/*.cache",
            "**/.DS_Store"
          ],
          "description": "Glob patterns to exclude from jumper tracking. Uses standard glob syntax (e.g., **/.git/**, **/node_modules/**)."
        },
        "jumper.debounceDelay": {
          "type": "number",
          "default": 500,
          "description": "Delay in milliseconds before tracking active editor changes. When rapidly switching files, only the final file is tracked after this delay."
        },
        "jumper.previewOnSelection": {
          "type": "boolean",
          "default": false,
          "description": "Preview files as you navigate through search results with arrow keys"
        },
        "jumper.openInNewTab": {
          "type": "boolean",
          "default": true,
          "description": "Open files in new tabs instead of reusing the preview tab"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "dependencies": {
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "eslint": "^8.0.0"
  },
  "publisher": "homerours",
  "repository": {
    "type": "git",
    "url": "https://github.com/homerours/jumper.vscode"
  },
  "keywords": [
    "jumper",
    "jump",
    "z",
    "navigation",
    "frecency",
    "fuzzy-finder",
    "file navigation",
    "quick open",
    "file finder",
    "productivity",
    "workspace",
    "recent files",
    "file search",
    "quick switch"
  ],
  "author": "homerours",
  "license": "MIT"
}
